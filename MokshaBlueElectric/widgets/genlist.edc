        name:           "event";
        type:           RECT;
        description {
           state: "default" 0.0;
           color: 0 0 0 0;
        }
     }
     part { name: "group";
       type: GROUP;
       source: "elm/genlist/tree/effect_parts/default";
       description {
         min: 16 28;
         state: "default" 0.0;
       }
       GENLIST_PART_MAP_START(-90.0, 0, 0)
       GENLIST_PART_MAP_FINISH(0, 0, 0)
