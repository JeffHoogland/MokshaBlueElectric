            rel1.offset: 5 3;
            rel2.to: "down_bt";
            rel2.offset: -6 -3;
            align: 0.5 0.5;
            image.normal: "down.png";
         }
      }
      part { name: "up_bt_icon";
         repeat_events: 1;
         description { state: "default" 0.0;
            rel1.to: "up_bt";
            rel1.offset: 5 3;
            rel2.to: "up_bt";
            rel2.offset: -6 -3;
            align: 0.5 0.5;
            image.normal: "up.png";
         }
      }
      part { name: "elm.text";
         type: TEXT;
         mouse_events: 0;
         scale: 1;
         description { state: "default" 0.0;
            visible: 1;
            align: 0.0 0.5;
            rel1 { relative: 0.0 0.0;
               offset: 3 2;
               to_x: "bg";
               to_y: "bg";
            }
            rel2 { relative: 0.0 1.0;
               offset: -3 -2;
               to_x: "down_bt";
               to_y: "bg";
            }
            color: 0 0 0 255;
            text {
               font: "Sans,Edje-Vera";
               size: 10;
